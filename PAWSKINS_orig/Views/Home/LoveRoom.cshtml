                                                <input type="text" />
                                            </div>
                                            <div class="profileFieldSave">
                                                <div class="saveButton">
                                                    <p>Сохранить</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="userTabs">
            <div class="transaction">
                <img src="~/Image/Dollars.png" />
                <label>
                    Транзакции
                </label>
            </div>
            <div class="inventoryLK">
                <div class="cubeLK">
                    <div class="cube123LK">
                        <img src="~/Image/1.png" class="cube1LK" />
                        <img src="~/Image/2.png" class="cube2LK" />
                        <img src="~/Image/3.png" class="cube3LK" />
                    </div>
                    <div class="cube456LK">
